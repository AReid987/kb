{
  "name": "TurboKBb",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint:gpt": "gptlint -c gptlint.config.mjs",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "pnpm exec --no-install devmoji -e"
    }
  },
  "lint-staged": {
    "*.{js, jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "pnpm format"
    ]
  },
  "devDependencies": {
    "devmoji": "^2.3.0",
    "gptlint": "^1.6.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "turbo": "^2.1.3",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "ip@<1.1.9": ">=1.1.9",
      "ip@=2.0.0": ">=2.0.1",
      "braces@<3.0.3": ">=3.0.3"
    }
  }
}
