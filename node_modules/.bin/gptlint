#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/antonioreid/devLand/knowledgeBoard/node_modules/.pnpm/gptlint@1.6.0/node_modules/gptlint/dist/bin/node_modules:/Users/antonioreid/devLand/knowledgeBoard/node_modules/.pnpm/gptlint@1.6.0/node_modules/gptlint/dist/node_modules:/Users/antonioreid/devLand/knowledgeBoard/node_modules/.pnpm/gptlint@1.6.0/node_modules/gptlint/node_modules:/Users/antonioreid/devLand/knowledgeBoard/node_modules/.pnpm/gptlint@1.6.0/node_modules:/Users/antonioreid/devLand/knowledgeBoard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/antonioreid/devLand/knowledgeBoard/node_modules/.pnpm/gptlint@1.6.0/node_modules/gptlint/dist/bin/node_modules:/Users/antonioreid/devLand/knowledgeBoard/node_modules/.pnpm/gptlint@1.6.0/node_modules/gptlint/dist/node_modules:/Users/antonioreid/devLand/knowledgeBoard/node_modules/.pnpm/gptlint@1.6.0/node_modules/gptlint/node_modules:/Users/antonioreid/devLand/knowledgeBoard/node_modules/.pnpm/gptlint@1.6.0/node_modules:/Users/antonioreid/devLand/knowledgeBoard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../gptlint/dist/bin/gptlint.js" "$@"
else
  exec node  "$basedir/../gptlint/dist/bin/gptlint.js" "$@"
fi
